{
  "mcpServers": {
    "windows-mcp": {
      "description": "MCP server for Windows system tasks like an USER",
      "command": "uvx",
      "args": [
        "windows-mcp"
      ]
    },
    "calculator-mcp": {
      "description": "MCP server for calculator tasks",
      "command": "uv",
      "args": [
        "--directory",
        "./mcp_servers",
        "run",
        "calculator_mcp.py"
      ]
    }
  }
}